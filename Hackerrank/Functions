#include <iostream>
#include <cstdio>
using namespace std;

/*
Add `int max_of_four(int a, int b, int c, int d)` here.
*/

int main() {
    int a, b, c, d;
    scanf("%d %d %d %d", &a, &b, &c, &d);
    if(a>b && a>c && a>d)
    {
        cout<<a;
    }
    else if(b>c && b>d && b>a)
    {
        cout<<b;
    }
    else if(c>d && c>a && c>b)
    {
        cout<<c;
    } 
    else
      {
          cout<<d;
      }
    return 0;
}
